<h1>Hello card</h1>
<div id="card">
{{#if crosses.length}}
<table>
    <thead>
        <tr>
            <th>Model</th>
            <th>Price</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
        {{#each crosses}}
        <tr>
            <td>{{model}}</td>
            <td>{{count}}</td>
            <td>
                <button class="btn btn-small js-remove" data-id="{{id}}">Delete</button>
            </td>
        </tr>
        {{/each}}
    </tbody>
</table>
<p><strong>Price:</strong><span class="price">{{price}}</span></p>
{{else}}
<p>basket is empty</p>
{{/if}}
</div>